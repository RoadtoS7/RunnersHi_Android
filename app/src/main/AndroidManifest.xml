<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.team.runnershi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.login.RecDetailActivity"
            android:label="@string/title_activity_rec_detail"></activity>
        <activity android:name=".ResultActivity" />
        <activity android:name=".CalcWinActivity" />
        <activity android:name=".FinishRunActivity" />
        <activity android:name=".RivalActivity" />
        <activity android:name=".GoalActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>