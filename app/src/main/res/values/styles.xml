<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="TextStyle3">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textSize">20sp</item>
        <item name="android:lineSpacingExtra">10sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="TextStyle">
        <item name="android:textSize">14sp</item>
        <item name="android:lineSpacingExtra">11sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="TextStyle2">
        <item name="android:textSize">12sp</item>
        <item name="android:lineSpacingExtra">12sp</item>
        <item name="android:textColor">@color/brown_grey</item>
    </style>

    <style name="TextStyle4">
        <item name="android:textSize">14sp</item>
        <item name="android:lineSpacingExtra">6sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="TextStyle6">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textSize">70sp</item>
        <item name="android:lineSpacingExtra">26sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="TextStyle5">
        <item name="android:gravity">end</item>
        <item name="android:textSize">16sp</item>
        <item name="android:lineSpacingExtra">2sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="TextStyle7">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textSize">16sp</item>
        <item name="android:lineSpacingExtra">2sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="TextStyle8">
        <item name="android:textSize">14sp</item>
        <item name="android:lineSpacingExtra">10sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="TextStyle9">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textSize">12sp</item>
        <item name="android:lineSpacingExtra">18sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


</resources>